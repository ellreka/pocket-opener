  "license": "MIT",
  "scripts": {
    "build": "yarn build:client && yarn build:server",
    "build:client": "WEBPACK_ENV=production webpack --config webpack/client.config.js",
    "build:server": "WEBPACK_ENV=production webpack --config webpack/server.config.js",
    "dev": "WEBPACK_ENV=development webpack-dev-server --config webpack/client.config.js",
    "start": "concurrently \"yarn build:client --watch\" \"yarn build:server && node dist/server/bundle.server.js\""
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
