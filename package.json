    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "3",
    "css-loader": "^3.2.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "forever": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "js-cookie": "^2.2.1",
    "lz-string": "^1.4.4",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3",
    "pocket-api": "^0.2.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "reactstrap": "^8.0.1",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-node-externals": "^1.7.2"
  }
}
